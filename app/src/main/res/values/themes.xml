<resources>
    <!-- Base application theme. -->
    <style name="Theme.TestSequenia" parent="Theme.Material3.Light.NoActionBar">
        <item name="colorPrimary">@color/gray_600</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="colorControlNormal">@color/white</item>
        <item name="colorPrimaryContainer">@color/blue_200</item>
        <item name="colorOnPrimaryContainer">@color/white</item>
        <item name="colorSecondary">@color/blue_400</item>
        <item name="colorOnSecondary">@color/white</item>
        <item name="colorSecondaryContainer">@color/blue_300</item>
        <item name="colorOnSecondaryContainer">@color/white</item>
        <item name="android:colorBackground">@color/gray_600</item>
        <item name="colorOnBackground">@color/white</item>
        <item name="colorSurface">@color/gray_400</item>
        <item name="colorOnSurface">@color/white</item>
        <item name="colorOnSurfaceVariant">@color/gray_200</item>
        <item name="colorOutline">@color/gray_700</item>
        <item name="colorOnSurfaceInverse">@color/white</item>

        <item name="shapeAppearanceSmallComponent">@style/ShapeAppearance.MyApp.SmallComponent
        </item>
        <item name="shapeAppearanceMediumComponent">@style/ShapeAppearance.MyApp.MediumComponent
        </item>
        <item name="shapeAppearanceLargeComponent">@style/ShapeAppearance.MyApp.LargeComponent
        </item>
